<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#39c234" />
            <gradient android:angle="-90" android:startColor="#107403" android:endColor="#22de41"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#299f1c" />
            <solid android:color="#15a11a"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#1cb611" />
            <gradient android:angle="-90" android:startColor="#2aff00" android:endColor="#206c11" />
        </shape>
    </item>
</selector>